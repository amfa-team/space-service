{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "es6",
    "target": "esnext",
    "lib": ["es2020", "dom"],
    "jsx": "react",
    "tsBuildInfoFile": "../../.build-cache/react.tsbuildinfo",
    "outDir": "./lib",
    "rootDir": "./src",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
