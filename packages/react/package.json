{
  "name": "@amfa-team/hello-service",
  "repository": "https://github.com/amfa-team/hello-service.git",
  "version": "0.1.0",
  "description": "React component library",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "lib/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "abortcontroller-polyfill": "^1.5.0",
    "core-js": "^3.7.0",
    "whatwg-fetch": "^3.5.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@saithodev/semantic-release-backmerge": "^1.2.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@sentry/react": "^5.27.3",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "concurrently": "^5.3.0",
    "cpx": "^1.5.0",
    "css-loader": "^5.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-modules-values": "^3.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "recoil": "^0.1.2",
    "rollup": "^2.33.1",
    "rollup-plugin-polyfill": "^3.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.2.2",
    "semantic-release-slack-bot": "^1.6.2",
    "style-loader": "^2.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5",
    "typescript-plugin-css-modules": "^2.8.0"
  },
  "peerDependencies": {
    "@amfa-team/hello-service-shared": "^0.1.0",
    "@sentry/react": "^5.27.3",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "recoil": "^0.1.2"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "scripts": {
    "build": "yarn build:tsc && yarn build:css && yarn build:rollup",
    "build:tsc": "tsc -b",
    "build:rollup": "rollup -c rollup.config.js",
    "build:css": "cpx 'src/**/*.css' lib",
    "clean": "tsc -b --clean && rm -rf dist",
    "start": "yarn build:tsc && yarn build:css && concurrently \"yarn start:tsc\" \"yarn start:rollup\" \"yarn start:css\"",
    "start:rollup": "yarn build:rollup -w",
    "start:tsc": "yarn build:tsc -w",
    "start:css": "yarn build:css --watch",
    "deploy": "semantic-release",
    "prepublishOnly": "yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
